{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Personal portfolio website built with Astro",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:production": "NODE_ENV=production astro check && astro build",
    "build:analyze": "astro build --analyze",
    "start:production": "NODE_ENV=production astro preview --host 0.0.0.0 --port 3000",
    "deploy:prepare": "npm run check && npm run build:production",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "test": "vitest --run"
  },
  "keywords": [
    "astro",
    "portfolio",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/vercel": "^8.2.2",
    "@vercel/analytics": "^1.5.0",
    "astro": "^5.12.0",
    "sharp": "^0.34.3",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "^24.0.14",
    "@vitest/ui": "^3.2.4",
    "chrome-launcher": "^1.2.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "puppeteer": "^24.14.0",
    "vitest": "^3.2.4"
  }
}
